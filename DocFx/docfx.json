{
    "metadata": [
        {
            "src": [
                {
					"files": [ "**/**.csproj" ],
					"src": "../../CoreProject"
                }
            ],
            "dest": "reference",
            "disableGitFeatures": false,
            "disableDefaultFilter": false,
            "filter": "filterConfig.yml"
        }
    ],
  "build": {
    "content": [
      {
        "files": [
          "reference/**.yml",
          "reference/index.md"
        ]
      },
      {
        "files": [
          "Concepts/toc.yml",
          "Concepts/**.md",
          "Concepts/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [ "images/**" ]
      }
    ],
    "dest": "../docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [ "default", "templates/singulinkfx" ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}